
@{
    ViewBag.Title = "History";
}

<!-- Page Title -->
<div class="row">
    <div class="col-xs-12">
        <h2 class="text-center">@ViewBag.Title</h2>
    </div>
</div>

<!-- We do a virtual list. Each row is a timeline element. -->

<!-- JustIT -->
<div class="row">
    <div class="col-xs-12 col-md-offset-1 col-md-10 well">
        <!-- Logo -->
        <div class="row">
            <div class="col-xs-12">
                <img src="http://dummyimage.com/600x200/000/fff" class="img-responsive center-block" alt="JustIT Logo" />
            </div>
        </div>
        <!-- Date -->
        <div class="row">
            <div class="col-xs-6">
                <h1 class="text-center">
                    <time datetime="2015-09">September 2015</time>
                </h1>
            </div>
            <div class="col-xs-6">
                <h1 class="text-center">
                    <time datetime="now">Now</time>
                </h1>
            </div>
        </div>
        <!-- Place/Organization -->
        <div class="row">
            <div class="col-xs-12">
                <h1 class="text-center">JustIT</h1>
            </div>
        </div>
        <!-- Job/Role -->
        <div class="row">
            <div class="col-xs-12">
                <h2 class="text-center">The Development Professional Program</h2>
            </div>
        </div>
        <!-- Description -->
        <div class="row">
            <div class="col-xs-12">
                <p><a href="http://app.pluralsight.com/profile/arnoldas-rudzevicius">Pluralsight Profile</a></p>
            </div>
        </div>
    </div>
</div>

